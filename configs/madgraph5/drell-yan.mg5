# MadEvent options
# 0=single core, 1=cluster, 2=multicore
set run_mode 2
set nb_core 6
generate p p > l+ l-
output drell-yan_output
launch drell-yan_output
# Set run card options
  # Use PYTHIA8 for the shower/hadronization
  shower=Pythia8
  # Generate 1.0e+06 events
  set nevents 1000000
  # Set random seed
  set iseed 0
