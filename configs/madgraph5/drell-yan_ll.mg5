# MadEvent options
# 0=single core, 1=cluster, 2=multicore
set run_mode 2
set nb_core 10
generate p p > l+ l-
output drell-yan_ll_output
launch drell-yan_ll_output
# Set run card options
  # Use PYTHIA8 for the shower/hadronization
  shower=Pythia8
  # Generate 5.0e+03 events
  set nevents 5000
  # Set random seed
  set iseed 0
  # Set cut values
  set ptl 25
  set etal 2.5
